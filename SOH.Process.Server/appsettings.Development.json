{
  "ApiBaseUrl": "https://localhost:7003/",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Error",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Enrich": [
      "WithExceptionDetails"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "OgcSettings": {
    "Capabilities": {
      "title": "SmartOpenHamburg OGC Processes API",
      "description": "Official OGC Processes API to execute the SmartOpenHamburg MARS simulation 1.0"
    }
  }
}
